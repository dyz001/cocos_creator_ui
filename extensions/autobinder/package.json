{
    "package_version": 2,
    "version": "1.0.0",
    "name": "autobinder",
    "description": "i18n:autobinder.description",
    "main": "./dist/main.js",
    "devDependencies": {
        "@types/node": "^16.0.1",
        "typescript": "^4.3.4"
    },
    "contributions": {
        "scene": {
            "script": "./dist/scene.js"
        },
        "messages": {
            "run":{
                "public": false,
                "methods":["run"]
            },
            "create_control":{
                "public":false,
                "methods":["creatControl"]
            }
        },
        "shortcuts": [
            {
                "message": "run",
                "win": "alt+g",
                "mac": "alt+g"
            }
        ],
        "menu":[
            {
                "path": "i18n:autobinder.custom_extensions",
                "label": "i18n:autobinder.create_control",
                "message": "create_control"
            }
        ]
    },
    "author": "greedy",
    "editor": ">=3.8.0",
    "scripts": {
        "build": "tsc -b",
        "watch": "tsc -w"
    }
}
